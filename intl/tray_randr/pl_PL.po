#: randr.c:78
msgid "Switch to external display"
msgstr "Przełącz na zewnętrzny monitor"

#: randr.c:79
msgid "Switch to built-in display"
msgstr "Przełącz na wbudowany monitor"

#: randr.c:80
msgid "Copy the flatpanel on the external screen"
msgstr "Kopiuj flatpanel na zewnętrzny monitor"

#: randr.c:81
msgid "Use external screen to extend the panel on the left"
msgstr "Użyj zewnętrznego monitora do powiększenia panela po lewo"

#: randr.c:82
msgid "Use external screen to extend the panel on the right"
msgstr "Użyj zewnętrznego monitora do powiększenia panela po prawo"

#: randr.c:88
msgid "Set ext. resolution to 1024x768"
msgstr "Ustaw zew. Rozdzielczość na  1024x768"

#: randr.c:89
msgid "Set ext. resolution to 800x600"
msgstr "Ustaw zew. Rozdzielczość na  800x600"

#: randr.c:90
msgid "Set ext. resolution to 640x480"
msgstr "Ustaw zew. Rozdzielczość na 640 x 480"

#: randr.c:99
msgid "Quit"
msgstr "Zamknij"

