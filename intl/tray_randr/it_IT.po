#: randr.c:78
msgid "Switch to external display"
msgstr "Riproduci su display esterno"

#: randr.c:79
msgid "Switch to built-in display"
msgstr "Riproduci su display integrato"

#: randr.c:80
msgid "Copy the flatpanel on the external screen"
msgstr "Copiare il lavoro su uno schermo esterno"

#: randr.c:81
msgid "Use external screen to extend the panel on the left"
msgstr "Utilizzo di uno schermo esterno - sinistra"

#: randr.c:82
msgid "Use external screen to extend the panel on the right"
msgstr "Utilizzo di uno schermo esterno - destra"

#: randr.c:88
msgid "Set ext. resolution to 1024x768"
msgstr "Selezionare risoluzione 1024x768"

#: randr.c:89
msgid "Set ext. resolution to 800x600"
msgstr "Selezionare risoluzione 800x600"

#: randr.c:90
msgid "Set ext. resolution to 640x480"
msgstr "Selezionare risoluzione 640x480"

#: randr.c:99
msgid "Quit"
msgstr "Esci"

